{
  "name": "@webpod/ip",
  "version": "0.6.1",
  "description": "Lite IP tools",
  "type": "module",
  "main": "target/cjs/index.cjs",
  "module": "target/esm/index.mjs",
  "types": "target/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./target/dts/index.d.ts",
      "browser": "./target/esm/core.mjs",
      "require": "./target/cjs/index.cjs",
      "import": "./target/esm/index.mjs",
      "default": "./target/esm/index.mjs"
    },
    "./core": {
      "types": "./target/dts/core.d.ts",
      "require": "./target/cjs/core.cjs",
      "import": "./target/esm/core.mjs",
      "default": "./target/esm/core.mjs"
    },
    "./lib/ip.js": {
      "types": "./target/dts/index.d.ts",
      "require": "./target/cjs/index.cjs"
    }
  },
  "files": [
    "target/cjs",
    "target/esm",
    "target/dts",
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "ip",
    "ip-address",
    "cidr",
    "subnet",
    "ipv4",
    "ipv6"
  ],
  "homepage": "https://github.com/webpod/ip#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpod/ip.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "Anton Golub <antongolub@antongolub.com>"
}
